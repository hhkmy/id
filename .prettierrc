{
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "parser": "go-template"
      }
    },
    {
      "files": ["*.html", "*.css"],
      "options": {
        "singleQuote": false
      }
    }
  ],
  "singleQuote": true,
  "printWidth": 250,
  "goTemplateBracketSpacing": true,
  "plugins": ["prettier-plugin-go-template"]
}
